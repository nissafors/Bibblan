
@{
    ViewBag.Title = "Author";
}

@using (Html.BeginForm("Title", "Browse"))
{
    <input type="submit" class="btn btn-default" value="Visa alla titlar" />
}
<div class="page-header">
    <h1>Alla författare</h1>
</div>

<table class="table table-striped">
    <tr>
        <th>Efternamn</th>
        <th>Förnamn</th>
        <th>Födelseår</th>
    </tr>
    @for (int i = 0; i < 200; i++)
    {
        <tr>
            <td>@Html.ActionLink("Bore", "Results", "Search")</td>
            <td>@Html.ActionLink("Rob", "Results", "Search")</td>
            <td>@(i + 1900)</td>
        </tr>
    }
</table>